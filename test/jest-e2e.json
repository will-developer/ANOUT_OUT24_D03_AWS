{
  "moduleFileExtensions": ["js", "json", "ts"],
  "testPathIgnorePatterns": ["/node_modules/', '/dist/"],
  "rootDir": "../",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1",
    "^prisma/(.*)$": "<rootDir>/src/prisma/$1"
  }
}


